{
  "name": "testing-react-workshop",
  "version": "1.0.0",
  "description": "Testing React Workshop allows to learn how to test react application -- without struggle.",
  "keywords": [
    "react",
    "testing-library-react",
    "jest"
  ],
  "scripts": {
    "dev": "node ./config/server/server.js",
    "test": "jest --config=./config/jest/jest.config.ts",
    "test:watch": "jest --config=./config/jest/jest.config.ts --watch --verbose",
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "Katarzyna Ziomek-Zdanowicz <katarzyna@zdanowicz.org.pl>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ziomecka/testing-react-workshop.git"
  },
  "license": "CC-BY-NC-ND",
  "dependencies": {
    "express": "4.17.1",
    "lorem-ipsum": "2.0.3",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@types/eslint": "7.2.6",
    "@types/lorem-ipsum": "2.0.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/webpack-env": "1.16.0",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "clean-webpack-plugin": "3.0.0",
    "eslint": "7.15.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-standard": "5.0.0",
    "html-webpack-plugin": "4.5.0",
    "jest": "26.6.3",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.11",
    "ts-node": "9.0.0",
    "typescript": "4.1.2",
    "webpack": "5.9.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-middleware": "4.0.2",
    "webpack-dev-server": "3.11.0",
    "webpack-hot-middleware": "2.25.0"
  },
  "engines": {
    "node": ">=12.13.1",
    "npm": ">=6.12.1"
  }
}
